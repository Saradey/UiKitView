<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleChartView">
        <attr name="arc_pie_space" format="dimension" />
        <attr name="arc_pie_stroke_size" format="dimension" />
        <attr name="arc_pie_mode" format="enum">
            <enum name="stroke" value="0" />
            <enum name="fill" value="1" />
        </attr>
        <attr name="arc_pie_elevation" format="boolean" />
    </declare-styleable>

</resources>