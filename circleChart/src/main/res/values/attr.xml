<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleChartView">
        <attr name="arc_pieSpace" format="dimension" />
        <attr name="arc_pieStrokeSize" format="dimension" />
        <attr name="arc_pieMode" format="enum">
            <enum name="stroke" value="0" />
            <enum name="fill" value="1" />
        </attr>
        <attr name="arc_pieElevation" format="boolean" />
    </declare-styleable>
</resources>